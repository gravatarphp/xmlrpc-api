{
    "name": "gravatarphp/xmlrpc-api",
    "description": "Access your Gravatar account through XML-RPC API",
    "license": "MIT",
    "keywords": ["gravatar", "xmlrpc"],
    "homepage": "https://gravatarphp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "lstrojny/fxmlrpc": "~0.9"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Gravatar\\Xmlrpc\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
